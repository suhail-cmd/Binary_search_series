#python
arr = [2,3,4,7,11]
k = 5 
for i in range(len(arr)):
    if arr[i] <= k:
        k += 1 
    else:
        break
print(k)
###############################
def kth_missing(arr,n,k):
    low,high = 0 , n - 1 
    while low <= high:
        mid = (low + high)//2
        missing = arr[mid] - (mid + 1) 
        if missing  < k:
            low = mid + 1 
        else:
            high = mid - 1 
    return low + k 
arr = [2,3,4,7,11,13]
k = 8
n = len(arr)
print(kth_missing(arr,n,k))
