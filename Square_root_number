#python
n = int(input())
ans = -1 
for i in range(1,n - 1):
    if i * i <= n:
        ans = i 
    else:
        break
print(ans)
########################
def square_root(n):
    low,high = 0,n
    ans = -1 
    while low <= high:
        mid = (low + high)//2
        if mid * mid <= n:
            ans = mid
            low = mid + 1 
        else:
          high = mid - 1 
    return ans
n = int(input())
print(square_root(n))
